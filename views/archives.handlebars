<h1>Archived Invoices</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">Archived By</th>
            <th scope="col">Invoice Name</th>
            <th scope="col">Invoice Link</th>
            <th scope="col">Paid</th>
            <th scope="col">due_date</th>

        </tr>
    </thead>
    <tbody>
        {{#each invoice}}
            <tr>
                <th scope="row">{{user_id}}</th>
                <th>{{name}}</th>
                <td><a href="/dashboard/invoice/{{id}}">Open Invoice</a></td>
                <td>{{is_paid}}</td>
                <td>{{format_date due_date}}</td>

            </tr>
        {{/each}}

    </tbody>
</table>